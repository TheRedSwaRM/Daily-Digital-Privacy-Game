; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project - Privacy"
run/main_scene="res://scenes/title.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/viewport_width=1024
window/size/viewport_height=576
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/custom_font="res://assets/font/ark-pixel-10px-monospaced-latin.ttf"
theme/default_font_antialiasing=0
theme/default_font_subpixel_positioning=0

[internationalization]

locale/translations_pot_files=PackedStringArray("res://assets/dialogue/sample.dialogue")

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/main_scene="res://scenes/main.tscn"
